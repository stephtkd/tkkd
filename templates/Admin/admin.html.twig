{% extends 'base.html.twig' %}{% block title %}Administration du site{% endblock %}{% block body %}    <div class="d-flex justify-content-center">        {# { form(my_form) }} #}        {{ form_start(my_form) }}        {{ form_errors(my_form) }}        <div class="form-group">            {{ form_label(my_form.name) }}            {{ form_widget(my_form.name, {'attr':                {'class':'form-control'}}) }}            {{ form_errors(my_form.name) }}        </div>        <div class="form-group">            {{ form_label(my_form.price) }}            {{ form_widget(my_form.price, {'attr':                {'class': 'form-control'}}) }}            {{ form_errors(my_form.price) }}        </div>    <div class="form-group">        {{ form_label(my_form.linkImage) }}        {{ form_widget(my_form.linkImage, {'attr':            {'class': 'form-control'}}) }}        {{ form_errors(my_form.linkImage) }}    </div>    {{ form_row(my_form.add, {'attr': {'class':'btn btn-info'}}) }}    {{ form_end(my_form) }}    </div>{% endblock %}